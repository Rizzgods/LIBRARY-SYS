{
  "version": 2,
  "builds": [
    {
      "src": "storefront/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" },
      "buildCommand": "pip install django-import-export"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "storefront/wsgi.py"
    }
  ]
}