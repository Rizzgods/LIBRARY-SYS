{
  "version": 2,
  "builds": [
    {
      "src": "storefront/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "storefront/wsgi.py"
    }
  ],
  "buildCommand": "pip install -r requirements.txt"
}